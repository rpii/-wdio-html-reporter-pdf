{
  "name": "@rpii/wdio-html-reporter-pdf",
  "version": "7.0.1",
  "description": "WebdriverIO report plugin. Converts HTML formatted report to pdf. compatible with webdriverio version 6",
  "main": "lib/printPdf.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./lib ./reports ./logs",
    "clean:build": "rimraf ./lib",
    "clean:reports": "rimraf ./reports",
    "test": "mocha --timeout 10000 ./test/**/*.ts",
    "symlink": "npm link",
    "use:symlink": "npm link @rpii/wdio-html-reporter-pdf"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rpii/wdio-html-reporter-pdf.git"
  },
  "directories": {
    "src": "./src"
  },
  "keywords": [
    "html",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-reporter",
    "wdio-html-reporter",
    "wdio-html-format-reporter",
    "wdio-html-format-reporter-pdf",
    "pdf"
  ],
  "author": "Rich Peters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rpii/wdio-html-reporter-pdf/issues"
  },
  "homepage": "https://github.com/rpii/wdio-html-reporter-pdf#readme",
  "dependencies": {
    "copyfiles": "^2.4.1",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "puppeteer": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/mocha": "^8.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5",
    "log4js": "^6.3.0",
    "mocha": "^9.0.2",
    "rimraf": "^3.0.2"
  },
  "contributors": [
    "Rich Peters <16782353+rpii@users.noreply.github.com>"
  ]
}
